<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="perceptron.TestingPanelController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" prefHeight="431.0" prefWidth="600.0" spacing="5.0">
         <children>
            <Label text="Testing Panel">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <HBox prefHeight="361.0" prefWidth="600.0" spacing="8.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="358.0" prefWidth="301.0">
                     <children>
                        <Label text="Inputs" />
                        <ScrollPane prefHeight="330.0" prefWidth="301.0">
                           <content>
                              <VBox fx:id="inputs_VBox" prefHeight="328.0" prefWidth="284.0" />
                           </content>
                        </ScrollPane>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" prefHeight="358.0" prefWidth="300.0">
                     <children>
                        <Label text="Outputs" />
                        <ScrollPane prefHeight="332.0" prefWidth="300.0">
                           <content>
                              <VBox fx:id="outputs_VBox" prefHeight="329.0" prefWidth="283.0" />
                           </content>
                        </ScrollPane>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#goTesting" text="Test" />
                  <Button mnemonicParsing="false" onAction="#goTraining" text="Train" />
                  <Button mnemonicParsing="false" onAction="#goHome" text="Home" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="580.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#savePerceptron" text="Save Perceptron" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
